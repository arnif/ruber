<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="ruberService" class="is.ru.honn.ruber.service.RuberServiceStub" >
        <constructor-arg ref="user"></constructor-arg>
    </bean>
    <bean id="user" class="is.ru.honn.ruber.domain.User">
        <property name="email"><value>foo@foo.is</value></property>
        <property name="firstName"><value>FooBar</value></property>
        <property name="userName"><value>foo</value></property>
        <property name="password"><value>bar</value></property>
        <property name="picture"><value>kool</value></property>
        <property name="promoCode"><value>123</value></property>

    </bean>

</beans>
